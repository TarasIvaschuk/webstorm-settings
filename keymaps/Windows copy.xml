<keymap version="1" name="Windows copy" parent="$default">
  <action id="ExpandLiveTemplateCustom">
    <keyboard-shortcut first-keystroke="quote" />
  </action>
  <action id="ExpandRegion">
    <keyboard-shortcut first-keystroke="ctrl add" />
    <keyboard-shortcut first-keystroke="ctrl equals" />
    <keyboard-shortcut first-keystroke="alt minus" />
  </action>
  <action id="GotoNextBookmark">
    <keyboard-shortcut first-keystroke="alt h" />
  </action>
  <action id="GotoPreviousBookmark">
    <keyboard-shortcut first-keystroke="alt t" />
  </action>
</keymap>